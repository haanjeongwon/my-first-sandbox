doctype html
html
  head
    meta(charset='UTF-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    title Parallex Scroll | Sandbox
    link(rel='icon' href='../../assets/images/favicon.svg')
    link(rel='icon' href='../../assets/images/favicon.png')
    link(rel='apple-touch-icon' href='../../assets/images/favicon.png')
    link(rel='stylesheet' href='../../assets/styles/common.css')
    link(rel='stylesheet' href='../../assets/styles/removescrollbar.css')
    script(src='../../assets/scripts/cssbrowserselector.js')
    script(src='./assets/scripts/script.js' defer)
  body#pp-001
    header
      div.inner
        a#logo(href='../../')
          embed(src='../../assets/images/logo.svg' type='image/svg+xml' aria-label='')
        h1 Parallax Scroll
    main
      section#front.parallax(data-speed='1.4')
        div.part
        div.part
        div.part
        div.part
        div.part
        div.part#hook-front
        div.part
        div.part
        div#attach-front
      section#back.parallax(data-speed='-0.5')
        div.part
        div.part
        div.part
        div.part
        div.part
        div.part#hook-back
        div.part
        div.part
        div#attach-back